{
  "name": "@kafaat-systems/schemaConfig",
  "version": "0.0.1",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "nx": {
    "name": "schemaConfig",
    "targets": {
      "build": {
        "executor": "@nx/js:tsc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "dist/libs/shared/schemaConfig",
          "tsConfig": "libs/shared/schemaConfig/tsconfig.lib.json",
          "packageJson": "libs/shared/schemaConfig/package.json",
          "main": "libs/shared/schemaConfig/src/index.ts",
          "assets": [
            "libs/shared/schemaConfig/*.md"
          ]
        }
      }
    }
  },
  "dependencies": {
    "tslib": "^2.3.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@kafaat-systems/entities": "^0.0.1",
    "typeorm": "^0.3.17",
    "express": "^4.18.2"
  }
}
